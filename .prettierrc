{
  "plugins": ["prettier-plugin-solidity"],
  "printWidth": 120,
  "trailingComma": "es5",
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "overrides": [
    {
      "files": "*.ts",
      "options": {
        "bracketSpacing": true
      }
    },
    {
      "files": "*.sol",
      "options": {
        "semi": true,
        "printWidth": 120
      }
    }
  ]
}
